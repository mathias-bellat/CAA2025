	year = {2018},
	keywords = {Lu, IDENTIFICATION, Machine learning, SITE, INFERRING FOSSIL HOMININ, Taphonomy, OLDUVAI GORGE, PERCUSSION MARKS, DIKIKA, CUT MARKS, Cut marks, Algorithm, Bone surface modification, MODIFICATION MODELS, MULTIVARIATE APPROACH, Trampling marks},
	pages = {2711--2725},
	file = {Successful classification of experimental bone surface modifications (BSM) through machine learning algorithms\: a solution to the controversial use of BSM in paleoanthropology?:C\:\\Users\\mathi\\Zotero\\storage\\AQB4QA45\\s12520-018-0684-9.pdf.pdf:application/pdf},
