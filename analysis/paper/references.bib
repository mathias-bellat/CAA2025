	month = jun,
	year = {2024},
	keywords = {Artificial intelligence, WHITE, MICRO-RAMAN, Artificial-aging, Binders, Dynamic light scattering, Egg, egg white, egg yok, Hen egg, MEDIA, Proteins, Raman Spectroscopy},
	pages = {77--85},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\6ZU76ARX\\Medhat et al. - 2024 - Raman spectroscopy and dynamic light scattering an.pdf:application/pdf},
