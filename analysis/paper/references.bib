	note = {Place: United States},
	keywords = {Lu, *Databases, Protein, *Tandem Mass Spectrometry, bioinformatics, de novo, forensics, mass spectrometry, metaproteomics, peptide identification, Peptides/*analysis, proteomics, unsequenced organisms},
	pages = {1864--1878},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\6RD8VQNP\\O'Bryon et al. - 2020 - Flying blind, or just flying under the radar The .pdf:application/pdf},
