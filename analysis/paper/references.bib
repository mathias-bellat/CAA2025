	number = {4},
	journal = {American journal of physical anthropology},
	author = {Hlad, Marta and Veselka, Barbara and Steadman, Dawnie Wolfe and Herregods, Baptiste and Elskens, Marc and Annaert, Rica and Boudin, Mathieu and Capuzzo, Giacomo and Dalle, Sarah and De Mulder, Guy and Sabaux, Charlotte and Salesse, Kevin and Sengeløv, Amanda and Stamataki, Elisavet and Vercauteren, Martine and Warmenbol, Eugène and Tys, Dries and Snoeck, Christophe},
