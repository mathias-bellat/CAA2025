	editor = {Watrobski, J. and Salabun, W. and Toro, C. and Zanni-Merk, C. and Howlett, R. J. and Jain, L. C.},
	year = {2021},
	note = {ISSN: 1877-0509
WOS:000720289002006},
	keywords = {deep learning, X-ray fluorescence, archaeology, chemical analysis, regression, restoration},
	pages = {2002--2011},
