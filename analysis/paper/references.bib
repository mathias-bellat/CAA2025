	note = {Place: Switzerland},
	keywords = {Deep Learning, LiDAR, photogrammetry, PHOTOGRAMMETRY, AERIAL, Remote Sensing, AREA, RADAR, COST, digital archaeology, digital preservation, fusion approaches, hidden features, INTEGRATION, MODELS, standalone approaches, TECHNOLOGIES, TERRESTRIAL},
