	author = {Kague, Erika and Turci, Francesco and Newman, Elis and Yang, Yushi and Brown, Kate Robson and Aglan, Mona S. and Otaify, Ghada A. and Temtamy, Samia A. and Ruiz-Perez, Victor L. and Cross, Stephen and Royall, C. Patrick and Witten, P. Eckhard and Hammond, Chrissy L.},
	month = aug,
	year = {2021},
	pmid = {34465741},
	pmcid = {PMC8408153},
	note = {Place: China},
	pages = {39},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\IHJNPYSH\\Kague et al. - 2021 - 3D assessment of intervertebral disc degeneration .pdf:application/pdf},
