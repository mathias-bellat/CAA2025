	keywords = {MODEL, ARCHAEOLOGY, REGRESSION, SITES, SIZE, DISTANCE, distance-decay, microblade core technology, obsidian sourcing, ORGANIZATION, PLS-DA, PORTABLE XRF, ROUTES, Southeast Alaska},
