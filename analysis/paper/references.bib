	pmcid = {PMC9080151},
	note = {Place: England},
	keywords = {Humans, Europe, Migration, *DNA, Mitochondrial/genetics, Phylogeny, Bayes Theorem, *Unsupervised Machine Learning, *Genetics, Population, Phylogeography, Clade, Evolution, Molecular, Haplotype, Haplotypes, Scandinavia},
	pages = {354},
