	keywords = {artificial intelligence (AI), media art, postdigital avant-garde, posthumanism, programmatic manifest, Web 3.0},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\PUN5DIHE\\Fernández-Castrillo - 2024 - Machinolatry and posthuman creativity between futu.pdf:application/pdf},
}

