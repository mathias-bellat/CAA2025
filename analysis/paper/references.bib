	pages = {143--154},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\YVV9HLKQ\\Verschoof-van der Vaart et Landauer - 2021 - Using CarcassonNet to automatically detect and tra.pdf:application/pdf},
