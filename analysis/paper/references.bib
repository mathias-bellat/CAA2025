@misc{bellat_what_2022,
	title = {What does matter? {Machine} {Learning} models to define archaeological patterns of site occupations and resource use in {Kurdistan} ({Iraq}) through a diachronic approach},
	copyright = {All rights reserved},
	shorttitle = {What does matter?},
	url = {https://hal.science/hal-03551422},
	abstract = {Mesopotamia is often seen as a region limited in resources, for example wood and minerals, while its peripheries Taurus, Zagros Mountains their foothills and the Levant region are very resourceful. This traditional vision led to the concept of “resource hunting” in which Mesopotamian empires would have spread to these regions to monopolize their resources. SFB 1070 aims to define new patterns by including resources in a wider spectrum as cultural elements not only for Mesopotamian empires but also for local communities. This new concept is called ResourceCultures. We investigate these issues in an archaeological and geoarchaeological survey covering an area of 4.400 km2 from the western foothills of the Zagros mountains to the eastern riverbank of the Tigris.

We analyzed more than 500 archaeological sites or installations detected in the survey zone from 2013 to 2019 by the German expedition of the SFB 1070 to unravel the patterns that determine a ResourceCulture and its underlying complex relation of resources. In order to find which features (water, topography, soil type, resources availability, vegetation, and others) influenced the founding and location of settlements, we analyze the feature importance for different landscape zones and time periods. To do so, we apply machine learning models inspired by predictive archaeology. These models will potentially help to better understand how resources were used in a cultural context through time and space.

This poster aims to present preliminary ideas derived from the machine learning approach: What trends appear and what are their possible interpretations? How can we use these interpretations to define the ResourceCultures in Northern Mesopotamia?},
	urldate = {2024-05-31},
	author = {Bellat, Mathias and Glissmann, Benjamin and Rentschler, Tobias and Paola, Sconzo and Pfläzner, Peter and Scholten, Thomas},
	month = mar,
	year = {2022},
	note = {Publisher: AFEQ
Published: AFEQ - CNF INQUA international conference 14 - 18 Mars 2022 : “ Q13 - Palaeoclimate changes, landscape evolution and human societies: from sedimentary basins to industrial landscapes ”},
	keywords = {Archéologie, Geoarchaeology, archaeology, Machine Learning, Archéologie du paysage, Apprentissage automatique, Landscape Archaeology, Geoarchaeologie, Near East Archaeology, Proche Orient, Science du sol, Soil Science},
}

@article{fernee_rolling_2022,
	title = {The rolling stones of {Bronze} {Age} {Aegean}: {Applying} machine learning to explore the use of lithic spheres from {Akrotiri}, {Thera}},
	volume = {45},
	issn = {2352-409X},
	doi = {10.1016/j.jasrep.2022.103615},
	abstract = {Across the Bronze Age Aegean and Eastern Mediterranean, various small spherical lithic artefacts have been discovered. The exact use of such artefacts remains a mystery, possible interpretations suggest that they are a form of counting system or counters for board games. This paper examines the largest collection of such artefacts found in the area to-date, the 746 spheres from the prehistoric town of Akrotiri in the island of Thera (Santorini). After the spheres were assessed and traditional statistical analyses were applied, machine learning was employed to further examine clustering of the spheres and to suggest potential interpretations of their manufacture, use and deposition. The paper concludes with the suggestion that the workflow proposed here, for Akrotiri, can be used to examine patterns in similar artefact groups from the Easter Mediterranean and beyond.},
	language = {English},
	journal = {JOURNAL OF ARCHAEOLOGICAL SCIENCE-REPORTS},
