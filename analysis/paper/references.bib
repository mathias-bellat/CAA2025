	keywords = {ANGULAR-MOMENTUM TRANSPORT, CHROMOSPHERIC VARIATIONS, DATA RELEASE, DIFFERENTIAL ROTATION, FUNDAMENTAL PROPERTIES, INTERNAL GRAVITY-WAVES, MAIN-SEQUENCE STARS, PHOTOMETRIC ACTIVITY, REVISED STELLAR PROPERTIES, SURFACE ROTATION},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\4A625ZUD\\Mathur et al. - 2023 - Magnetic Activity Evolution of Solar-like Stars. I.pdf:application/pdf},
}

