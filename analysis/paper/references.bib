	keywords = {RECOGNITION, Feature extraction, DIRECTIONS, Feature combination, FIRE, Phytolith classification, SEDIMENTS, Stacking},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\9Q7FWLMK\\Díez-Pastor et al. - 2024 - Towards automatic phytolith classification using f.pdf:application/pdf},
