	keywords = {machine learning, random forest, feature importance, Non-communicable disease, partial dependence, prediction, socioeconomic status},
	pages = {14034948241249519},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\2ZIAY4D9\\Svalestuen et al. - 2024 - Exploring the variation in associations between so.pdf:application/pdf},
