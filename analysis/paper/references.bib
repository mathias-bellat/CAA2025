	keywords = {REGRESSION, ATMOSPHERIC PARAMETERS, DISTANCES, DWARF, GIANT STARS, methods: data analysis, MILKY, MILLION FGK STARS, PHOTOMETRIC METALLICITIES, SDSS, stars: abundances, stars: fundamental parameters, surveys, techniques: photometric},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\WX6H8Y46\\Yang et al. - 2022 - J-PLUS Stellar parameters, C, N, Mg, Ca, and [alp.pdf:application/pdf},
