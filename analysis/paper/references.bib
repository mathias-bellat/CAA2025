	note = {Place: Ireland},
	keywords = {Machine learning, Humans, GIS, *Paleontology, *Postmortem Changes, Autopsy, Decomposition, Forensic Sciences, Longitudinal Studies, Open science, PMI},
	pages = {111934},
}

