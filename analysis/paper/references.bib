	author = {Yu, Yao and Yang, Xiaomin and Liu, Daiyun and Du, Panxin and Meng, Hailiang and Huang, Zixiao and Xiong, Jianxue and Ding, Yi and Ren, Xiaoying and Allen, Edward and Wang, Hui and Han, Sheng and Jin, Li and Wang, Chuan-Chao and Wen, Shaoqing},
