	keywords = {machine learning, remote sensing, Humans, History, Ancient, archaeology, *Archaeology, Nasca, geoglyphs},
	pages = {e2407652121},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\EIHS3QDY\\Sakai et al. - 2024 - AI-accelerated Nazca survey nearly doubles the num.pdf:application/pdf},
