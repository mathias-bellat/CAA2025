	year = {2019},
	keywords = {machine learning, Lu, notion, classification, ALGORITHM, cultural heritage, DOCUMENTATION, OBJECT DETECTION, POINT CLOUDS, random forest, segmentation, SURFACE MESH SEGMENTATION},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\DKD26J5T\\Grilli et Remondino - 2019 - Classification of 3D Digital Heritage.pdf:application/pdf},
