	pages = {7--35},
	file = {Garcia-Martinez_2022.pdf:G\:\\OneDrive\\Ecole\\Articles\\Article_ML\\PDF_Files\\6th_research\\Web_science\\Garcia-Martinez_2022.pdf:application/pdf},
