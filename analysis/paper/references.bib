	keywords = {machine learning, GEOMETRIC MORPHOMETRICS, OLDUVAI GORGE, CUT-MARKS, taphonomy, BONES, BRONZE, chalcolithic, EXPLOITATION, geometric morphometrics, metal slice marks, metallurgy, middle Tagus valley, STONE, stone tools slice marks},
	pages = {565--582},
