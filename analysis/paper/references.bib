	pmid = {37666948},
	note = {Place: England},
	keywords = {*Deep Learning, Genetics, Population, Genome, Biological Evolution, Genomics},
	pages = {61--78},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\28AM3PFW\\Huang et al. - 2024 - Harnessing deep learning for population genetic in.pdf:application/pdf},
}

