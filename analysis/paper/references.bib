@article{masini_uav_2022,
	title = {{UAV} {LiDAR} {Based} {Approach} for the {Detection} and {Interpretation} of {Archaeological} {Micro} {Topography} under {Canopy}-{The} {Rediscovery} of {Perticara} ({Basilicata}, {Italy})},
