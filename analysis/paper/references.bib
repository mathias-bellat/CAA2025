	keywords = {Humans, Animals, Machine Learning, *Ecosystem, Agriculture/*economics, Animal Husbandry, Animals, Domestic, Economics/*trends, Entropy, Food/economics, Societies/*economics},
	pages = {e0254539},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\YVDL4F42\\Ahedo et al. - 2021 - Let's go fishing A quantitative analysis of subsi.pdf:application/pdf},
