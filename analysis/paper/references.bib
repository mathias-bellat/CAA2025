	author = {Zwir, I and Del-Val, C and Hintsanen, M and Cloninger, KM and Romero-Zaliz, R and Mesa, A and Arnedo, J and Salas, R and Poblete, GF and Raitoharju, E and Raitakari, O and Keltikangas-Jarvinen, L and de Erausquin, GA and Tattersall, I and Lehtimaki, T and Cloninger, CR},
	month = jan,
	year = {2022},
	keywords = {ORIGINS, LIFE, ARCHAEOLOGICAL PERSPECTIVES, COVERAGE NEANDERTHAL GENOME, DEFAULT MODE, DIVERGENT-THINKING, MENTAL TIME-TRAVEL, MODERN HUMAN COLONIZATION, PERSONALITY, SEQUENCE},
	pages = {354--376},
