	keywords = {Taphonomy, Computer vision, Human evolution, Meat-eating, Paleoanthropology, Tooth marks},
	pages = {e14148},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\IBDM9LQL\\Cobo-Sánchez et al. - 2022 - Computer vision supports primary access to meat by.pdf:application/pdf},
