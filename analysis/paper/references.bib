	keywords = {Machine learning, Biome, FAPAR, Forest species, Global, Land potential, Natural vegetation, Random forest},
	pages = {e5457},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\X9CICN8P\\Hengl et al. - 2018 - Global mapping of potential natural vegetation an.pdf:application/pdf},
}

