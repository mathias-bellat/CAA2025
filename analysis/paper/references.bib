	month = jul,
	year = {2022},
	keywords = {Deep learning, Data augmentation, RECONSTRUCTION, 3D object completion, Archaeological fragments, Digital restoration, Self-Supervised learning},
	pages = {138--148},
	file = {Stasinakis et al. - 2022 - A hybrid 3D object auto-completion approach with s.pdf:C\:\\Users\\mathi\\Zotero\\storage\\S2ICE2DY\\Stasinakis et al. - 2022 - A hybrid 3D object auto-completion approach with s.pdf:application/pdf},
