	pmcid = {PMC11245623},
	note = {Place: England},
	keywords = {Machine learning, Archaeobotanical remains, CHARCOAL, Grapevine, Morphometric analysis, WASTE},
	pages = {16148},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\M4UZ93N4\\Landa et al. - 2024 - Setting the morphologic quality limits enabling ac.pdf:application/pdf},
