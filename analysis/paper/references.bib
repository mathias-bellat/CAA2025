@article{bonino_naval_2022,
	title = {Naval images, ideas and notes by {Leonardo} da {Vinci}},
	volume = {19},
	issn = {1724-6091},
	doi = {10.19272/202204501009},
	abstract = {Recent research makes it possible to study more precisely what Leonardo da Vinci drew and noted on naval subjects; thus, we can update and complete the proposals produced from 1939 to 1954, mainly by the naval engineer Luigi Tursini. Leonardo's notes, pictures, and drawings related to naval subjects are scattered and incomplete, but we can still identify some trends, which Leonardo followed fairly consistently, as in research programs. Leonardo had a deep self-taught culture; in the nautical field he knew the main classical sources and the Navis (now lost) by Leon Battista Alberti. He was also familiar with the naval machines and weapons proposed by his forerunners, but he developed his knowledge mainly through experiences, which he obtained through observations, interviews and tests. His observations on vessels were complementary to his research on the motions of water and air. He learned how to sail with a lateen sail, moreover he studied the shape of hulls and their resistance to water in relation to the growth of eddies and tacking of a sailboat in comparison to the flight of birds. He developed basic ideas on the best shapes of hulls even with a test tank, anticipating studies begun in the 18th and 19th centuries. Leonardo accurately recorded sea and river crafts of the Tyrrhenian coast, the Milanese shipping canals, the Adda, the Ticino, and Lake Maggiore. His drawings confirm that local Lombard crafts were generally similar to the traditional ones used until the early decades of the 20th century. Leonardo distinguished well the types of hulls, rudders, and rigging, and also made some notes on construction details, but he did not develop these elements; he drew construction details and general plans only for other projects and subjects, but for water crafts he drew only first-hand ideas, sketches that he perhaps planned to develop later. He could have jotted down other naval subjects, but most likely many of his notes were lost. He knew of a boat designed by Filippo Brunelleschi to carry marble and other heavy loads to Florence, but we do not know what it looked like, although some sketches circulated among scholars. Certainly, Leonardo saw and probably noted the parade boats used by the court of Ludovico il Moro around 1490. They were similar to the bucintoro built in 1475 by Luca Fancelli for the Duke of Mantua, a reconstruction of which is proposed here. This article reports the first part of the study; the forthcoming second part will deal of naval machines, common and special warships, underwater solutions and naval weapons.},
	language = {Italian},
	journal = {ARCHAEOLOGIA MARITIMA MEDITERRANEA-AN INTERNATIONAL JOURNAL ON UNDERWATER ARCHAEOLOGY},
	author = {Bonino, M},
	year = {2022},
	keywords = {15th Century, Leonardo da Vinci, Studies on Ships and Boats, the Artist's View of Watercrafts},
	pages = {167--230},
}

@article{alviz-meza_bibliometric_2022,
	title = {Bibliometric analysis of fourth industrial revolution applied to heritage studies based on web of science and scopus databases from 2016 to 2021},
	volume = {10},
	issn = {2050-7445},
	doi = {10.1186/s40494-022-00821-3},
	abstract = {Using past material and spiritual remains, cultural heritage examines communities' identity formation across time. Cultural heritage requires public and private institutions to care about its restoration, maintenance, conservation, and promotion. Through a bibliometric perspective, this study has analyzed, quantified, and mapped the scientific production of the fourth industrial revolution applied to heritage studies from 2016 to 2021 in the Scopus and Web of Science databases. Biblioshiny software from RStudio was employed to categorize and evaluate the contribution of authors, countries, institutions, and journals. In addition, VOSviewer was used to visualize their collaboration networks. As a main result, we found that augmented reality and remote sensing represent the research hotspot concerning heritage studies. Those techniques have become common in archaeology, as well as museums, leading to an increase in their activity. Perhaps, more recent tools, such as machine learning and deep learning, will provide future pathways in cultural heritage from data collected in social networks. This bibliometric analysis, therefore, provides an updated perspective of the implementations of technologies from industry 4.0 in heritage science as a possible guideline for future worldwide research.},
	language = {English},
