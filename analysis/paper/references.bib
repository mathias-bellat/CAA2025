	journal = {SCIRES-IT-SCIENTIFIC RESEARCH AND INFORMATION TECHNOLOGY},
	author = {Chaidron, C and Lermenier, S},
	year = {2022},
	keywords = {Lu, satellite images, SATELLITE IMAGERY, Archaeology, artificial intelligence, ceramology, multispectral imaging, roman pottery},
