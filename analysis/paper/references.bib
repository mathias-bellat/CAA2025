@article{thiery_semi-automatic_2023,
	title = {A {Semi}-{Automatic} {Semantic}-{Model}-{Based} {Comparison} {Workflow} for {Archaeological} {Features} on {Roman} {Ceramics}},
	volume = {12},
	issn = {2220-9964},
	doi = {10.3390/ijgi12040167},
	abstract = {In this paper, we introduce applications of Artificial Intelligence techniques, such as Decision Trees and Semantic Reasoning, for semi-automatic and semantic-model-based decision-making for archaeological feature comparisons. This paper uses the example of Roman African Red Slip Ware (ARS) and the collection of ARS at the LEIZA archaeological research institute. The main challenge is to create a Digital Twin of the ARS objects and artefacts using geometric capturing and semantic modelling of archaeological information. Moreover, the individualisation and comparison of features (appliques), along with their visualisation, extraction, and rectification, results in a strategy and application for comparison of these features using both geometrical and archaeological aspects with a comprehensible rule set. This method of a semi-automatic semantic model-based comparison workflow for archaeological features on Roman ceramics is showcased, discussed, and concluded in three use cases: woman and boy, human-horse hybrid, and bears with local twists and shifts.},
