@article{leskovar_atr-ftir_2020,
	title = {{ATR}-{FTIR} spectroscopy combined with data manipulation as a pre-screening method to assess {DNA} preservation in skeletal remains.},
	volume = {44},
	copyright = {Copyright © 2019 Elsevier B.V. All rights reserved.},
	issn = {1878-0326 1872-4973},
	doi = {10.1016/j.fsigen.2019.102196},
	abstract = {Skeletal remains are commonly subjected to various analyses, including DNA. As the remains are exposed to taphonomic processes after the death of the organism,  their physicochemical structure undergoes alterations. The success and integrity  of a DNA analysis is thus conditioned by the preservation state of the sample. In  this study, ATR-FTIR spectroscopy with further data exploration was employed to  characterize the physicochemical structure of the samples and its correlation  with the preservation state of the DNA. The aim was to test the hypothesis that  ATR-FTIR-obtained spectra contain enough information to allow classification of  the samples based on the preservation of the DNA in the remains. In the study,  138 human bones and teeth originating from the 16th century BC to the 21 st  century AD were used. The samples were cleaned and powdered following the  established methodological procedures for DNA extraction. DNA was extracted and  quantified. The samples were separated into four categories based on the amount  of quantified DNA. The remaining powder was analyzed with ATR-FTIR spectroscopy  and the spectra obtained were explored to extract physicochemical information.  Before the exploration of the acquired data, samples were divided into groups A  (n = 107) and B (n = 31). Statistical analyses and machine learning were  performed on the group A samples. The protocol was then validated on the group B  samples, which served to make predictions on the preservation of the DNA in the  remains. The best results were achieved using a random forest learning algorithm  employing either normalized spectra, second-derivative spectra, or five  highest-ranked ratios. Even though overlapping remained, these findings indicate  that ATR-FTIR spectroscopy with further exploration of the data has good  potential as a pre-screening method for evaluating DNA preservation in skeletal  remains.},
	language = {eng},
	journal = {Forensic science international. Genetics},
	author = {Leskovar, Tamara and Zupanič Pajnič, Irena and Geršak, Živa Miriam and Jerman, Ivan and Črešnar, Matija},
	month = jan,
	year = {2020},
	pmid = {31706110},
	note = {Place: Netherlands},
	keywords = {Humans, Bone and Bones/*chemistry, DIAGENESIS, EXTRACTION, Machine Learning, *Body Remains, CRYSTALLINITY, ARCHAEOLOGICAL BONE, Human skeletal remains, ATR-FTIR spectroscopy, BONE PRESERVATION, Pre-screening method, SURVIVAL, Principal Component Analysis, ANCIENT DNA, *Spectroscopy, Fourier Transform Infrared, DNA Degradation, Necrotic, DNA preservation, DNA/*analysis, FORENSIC GENETICS, Forensic Genetics/methods, INTERNATIONAL SOCIETY, Multiplex Polymerase Chain Reaction, Real-Time Polymerase Chain Reaction, RECOMMENDATIONS, Specimen Handling, Spectra exploration, Tooth/*chemistry},
	pages = {102196},
