	pages = {3327--3339},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\QWN3WVLM\\Firoze et al. - 2022 - Urban tree generator spatio-temporal and generati.pdf:application/pdf},
}

