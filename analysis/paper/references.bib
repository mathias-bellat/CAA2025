	keywords = {ASSOCIATION, CLOVIS POINT, Data analysis, Discriminant analysis, ESSENTIALISM STORY, EVOLUTION, LATE PLEISTOCENE, Machine learning, MAMMOTH, Morphometrics, North America, Paleogeography, Paleoindian, Projectile points, PROJECTILE POINTS, Quaternary, REMAINS, SITE, Typology, VARIABILITY},
	pages = {319--348},
