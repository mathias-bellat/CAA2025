	keywords = {machine learning, RECOGNITION, Segmentation, IMAGE, PETROGLYPHS, shape recognition},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\7XVG56LR\\Melnik et al. - 2022 - Deep Segmentation of Corrupted Glyphs.pdf:application/pdf},
}

