	keywords = {Lu, Machine learning, Taphonomy, Deep learning, Paleoanthropology, Cut mark, DIKIKA, ROCKS, SUPPORT VECTOR MACHINES, Trampling},
	pages = {36--43},
	file = {Automated identification and deep classification of cut marks on bones and its paleoanthropological implications:C\:\\Users\\mathi\\Zotero\\storage\\4XPNVNTK\\j.jocs.2019.02.005.pdf.pdf:application/pdf},
