	keywords = {RECOGNITION, NEURAL-NETWORKS, artificial intelligence, ECOLOGY, CLIMATE-CHANGE, DEEP, EPIDEMICS, INFECTIOUS-DISEASES, Kenya, KENYA, MALARIA TRANSMISSION, One Health, Rift Valley fever, RISK-FACTORS, slaughter},
	pages = {345--354},
	file = {Archaeology and contemporary emerging zoonosis\: A framework for predicting future Rift Valley fever virus outbreaks:C\:\\Users\\mathi\\Zotero\\storage\\LJLXW4E5\\oa.2862.pdf.pdf:application/pdf},
