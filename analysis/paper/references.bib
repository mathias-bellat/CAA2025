	month = may,
	year = {2023},
	keywords = {machine learning, CLASSIFICATION, random forest, glmnet, microdebitage analysis, Naive Bayes, XGBoost},
	pages = {152--163},
	file = {Full Text:C\:\\Users\\mathi\\Zotero\\storage\\9QQ4B9AR\\Eberl et al. - 2023 - Machine Learning-Based Identification of Lithic Mi.pdf:application/pdf},
}

