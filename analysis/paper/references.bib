	file = {Bennett et al. - 2025 - Guidelines for the use of Airborne Laser Scanning .pdf:E\:\\OneDrive\\Ecole\\Biblio\\Thèse\\G.I.S\\Bennett et al. - 2025 - Guidelines for the use of Airborne Laser Scanning .pdf:application/pdf},
