	keywords = {machine learning, LiDAR, EXTRACTION, UAV, ALGORITHMS, LANDSCAPE, LIGHT DETECTION, AIRBORNE LIDAR, deserted villages in the Middle Ages, FILTER, landslides},
	file = {Texte intégral:C\:\\Users\\mathi\\Zotero\\storage\\4HPFX4YN\\Masini et al. - 2022 - UAV LiDAR Based Approach for the Detection and Int.pdf:application/pdf},
