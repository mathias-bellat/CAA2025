	pages = {1000--1013},
	file = {Exploring Biological and Geological Age-related Changes through Variations in Intra- and Intertooth Proteomes of Ancient Dentine.:C\:\\Users\\mathi\\Zotero\\storage\\CM68LM3A\\acs.jproteome.7b00648.pdf.pdf:application/pdf},
