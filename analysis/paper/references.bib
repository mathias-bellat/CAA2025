	keywords = {SEARCH, Epistemology, ARTIFACTS, 1991-VG, ANTIMATTER, Astroengineering, Cosmic archaeology, Dysonian SETI, EXTRATERRESTRIAL CIVILIZATIONS, Extraterrestrial intelligence, Fermi's paradox, GALAXIES, History and philosophy of astronomy, INTELLIGENCE, Kardashev's classification, QUASARS, SETA, SPHERES, STELLAR},
	pages = {289--298},
	file = {Astroengineering, Dysonian SETI, and naturalism\: A new Catch-22:C\:\\Users\\mathi\\Zotero\\storage\\DVNR6Y5D\\j.actaastro.2018.07.051.pdf.pdf:application/pdf},
